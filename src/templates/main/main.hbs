<main class="container">
        <section class="car-intro">
            <div class="car-intro__name intro">
                <h1 class="intro__title">Model S</h1>
                <h2 class="car-intro_name-subtitle intro__subtitle">Добро пожаловать в будущее</h2>
            </div>
            <img src="img/tesla-with-logo.png" alt="Model S" class="car-intro__img">
            <div class="car-intro__features feature-details">
                <div class="params">
                    <img src="img/taho.svg" alt="" class="params__icon taho">
                    <div class="taho__info taho-specs">
                        <span class="taho-specs__speed">2.7 <span>сек</span> <br></span>
                        <span class="taho-specs__dist">0-100 км/час</span>
                    </div>
                </div>
                <div class="params path-param">
                    <img src="img/path.svg" alt="" class="params__icon path">
                    <div class="path__info path-specs">
                        <span class="path-specs__length">632 <span>км</span> <br></span>
                        <span class="path-specs__range">Запас хода</span>
                    </div>
                </div>
                <div class="params stear-icon">
                    <svg width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg" class="stear-icon__img">
